apiVersion: v1
kind: Service
metadata:
  name: {{ name }}
  {% if annotations is defined %}
  annotations:
    {% for annotation in annotations %}
    {{ annotaion.name }}: {{ annotaion.value }}
    {% endfor %}
  {% endif %}

spec:
  type: {{ type }}
  selector:
    app: {{ app }}
  ports:
    - name: {{ portname }}
      protocol: TCP
      port: {{ port }}
